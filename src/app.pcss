.app {
  width: 100vw;
  height: 100vh;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;

  & .title {
    color: #444;
    font-size: 40px;
    font-weight: 100;

    & .bold {
      font-weight: 600;
    }
  }

  & .copyright {
    margin-top: 20px;
    color: #ccc;
    font-size: 15px;
  }
}
